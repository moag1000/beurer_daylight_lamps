{
  "entity": {
    "button": {
      "identify": {
        "default": "mdi:flashlight"
      },
      "reconnect": {
        "default": "mdi:bluetooth-connect"
      }
    },
    "sensor": {
      "rssi": {
        "default": "mdi:signal"
      },
      "therapy_today": {
        "default": "mdi:sun-clock"
      },
      "therapy_week": {
        "default": "mdi:calendar-week"
      },
      "therapy_progress": {
        "default": "mdi:progress-check"
      }
    },
    "binary_sensor": {
      "connected": {
        "default": "mdi:bluetooth-connect",
        "state": {
          "on": "mdi:bluetooth-connect",
          "off": "mdi:bluetooth-off"
        }
      },
      "ble_available": {
        "default": "mdi:bluetooth-audio",
        "state": {
          "on": "mdi:bluetooth",
          "off": "mdi:bluetooth-off"
        }
      },
      "therapy_goal_reached": {
        "default": "mdi:check-circle-outline",
        "state": {
          "on": "mdi:check-circle",
          "off": "mdi:circle-outline"
        }
      }
    },
    "select": {
      "effect": {
        "default": "mdi:palette"
      }
    },
    "number": {
      "white_brightness": {
        "default": "mdi:brightness-6"
      },
      "color_brightness": {
        "default": "mdi:brightness-7"
      },
      "timer": {
        "default": "mdi:timer-outline"
      },
      "therapy_goal": {
        "default": "mdi:target"
      }
    },
    "switch": {
      "adaptive_lighting": {
        "default": "mdi:brightness-auto",
        "state": {
          "on": "mdi:brightness-auto",
          "off": "mdi:brightness-5"
        }
      }
    }
  },
  "services": {
    "apply_preset": "mdi:palette-swatch",
    "set_timer": "mdi:timer",
    "send_raw_command": "mdi:console",
    "start_sunrise": "mdi:weather-sunset-up",
    "start_sunset": "mdi:weather-sunset-down",
    "stop_simulation": "mdi:stop"
  }
}
